<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.befresh.befreshapp.Membership.MembershipWelcomeFragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="67.3dp"
            android:layout_height="107.3dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="81dp"
            android:src="@drawable/loginpage_logo" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30.5dp"
            android:gravity="center_horizontal"
            android:text="프레시 피플이 되신 것을 환영합니다." />


        <Button
            android:id="@+id/btn_membership_welcome"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:layout_alignParentBottom="true"
            android:background="@color/colormain"
            android:text="스케쥴 조정하기"
            android:textAlignment="center"
            android:textColor="#ffffff" />
    </RelativeLayout>
</FrameLayout>
