<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.befresh.befreshapp.Community.SaveList.SaveListFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.design.widget.TabLayout
            android:id="@+id/tab_save_list"
            android:layout_width="match_parent"
            app:tabSelectedTextColor="@color/colormain"
            app:tabIndicatorHeight="5dp"
            android:layout_height="?attr/actionBarSize"></android.support.design.widget.TabLayout>

        <android.support.v4.view.ViewPager
            android:id="@+id/pager_save_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></android.support.v4.view.ViewPager>

    </LinearLayout>
</FrameLayout>
